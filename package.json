{
  "name": "estime",
  "version": "1.22.2",
  "scripts": {
    "build:dev": "npm install && ng build",
    "build:prod": "npm ci && ng build --configuration production --build-optimizer",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:cache:list": "cypress cache list",
    "cy:run": "cypress run --config chromeWebSecurity=false",
    "cy:open": "cypress open --config chromeWebSecurity=false",
    "e2e": "cypress run",
    "e2e:record": "cypress run --record",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve --open --port 9001 --host 127.0.0.1",
    "start:expose": "ng serve --port 9001 --host 127.0.0.1 --disable-host-check --live-reload",
    "start:ngx": "ng run estime:serve-ngx --host 127.0.0.1 --port 80 --disable-host-check",
    "start:sudo": "sudo -E env \"PATH=$PATH\" ng serve --open --port 80 --host 127.0.0.1 --disable-host-check",
    "stats:size": "ng build --stats-json && webpack-bundle-analyzer dist/estime/stats.json",
    "test": "ng test",
    "sonar": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.6",
    "@angular/common": "13.3.6",
    "@angular/compiler": "13.3.6",
    "@angular/core": "13.3.6",
    "@angular/forms": "13.3.6",
    "@angular/platform-browser": "13.3.6",
    "@angular/platform-browser-dynamic": "13.3.6",
    "@angular/router": "13.3.6",
    "@fortawesome/fontawesome-free": "6.1.1",
    "angular2-chartjs": "0.5.1",
    "bn-ng-idle": "2.0.1",
    "bootstrap": "5.1.3",
    "chartjs-plugin-datalabels": "0.7.0",
    "html-to-pdfmake": "2.3.11",
    "image-to-base64": "2.2.0",
    "jquery": "3.6.0",
    "modele-social": "0.2.0",
    "moment": "2.29.3",
    "ngx-bootstrap": "8.0.0",
    "ngx-cookie-service": "13.2.0",
    "ngx-webstorage": "9.0.0",
    "pdfmake": "0.2.5",
    "publicodes": "1.0.0-beta.16",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "web-animations-js": "2.3.2",
    "zone.js": "0.11.5"
  },
